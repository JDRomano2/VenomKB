{
  "name": "venomkb",
  "version": "2.0.0-beta",
  "description": "A knowledge base for aggregating and identifying therapeutic uses of animal venoms and venom compounds.",
  "main": "venomkb.js",
  "scripts": {
    "start": "npm run dev",
    "api": "node ./index/bin/www",
    "dev_server": "node venomkb.server.js",
    "build": "rimraf dist && webpack --config ./webpack.config.js --progress --profile --colors",
    "build2": "rimraf dist && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "eslint": "eslint .",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "prod_server": "node ./server/index.js",
    "production": "concurrently \"npm run prod_server\" \"npm run api\"",
    "dev": "concurrently \"npm run dev_server\" \"npm run api\""
  },
  "keywords": [
    "venomkb",
    "venom",
    "es6",
    "react",
    "redux"
  ],
  "author": "Joseph D. Romano, Victor Nwankwo",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "css-loader": "^0.25.0",
    "express": "~4.13.4",
    "file-loader": "^0.9.0",
    "file-saver": "^1.3.8",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^4.13.12",
    "morgan": "~1.7.0",
    "rc-slider": "^8.6.1",
    "rc-tooltip": "^3.7.0",
    "react": "^15.6.2",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^15.6.2",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^4.4.9",
    "react-router": "^3.2.1",
    "react-router-redux": "^4.0.4",
    "react-select": "^1.2.1",
    "react-table": "^6.8.0",
    "react-tooltip": "^3.4.0",
    "react-virtualized": "^9.18.5",
    "react-virtualized-select": "^3.1.3",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "~2.3.0"
